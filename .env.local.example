# Server Configuration
PORT=3000

# E2E Test Configuration
E2E_PORT=3001

# NextAuth Configuration
# URL of your application (used for callback URLs and authentication)
NEXTAUTH_URL="http://localhost:3000"

# Secret for JWT encryption
# Generate with: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here"

# API Keys
# Kakao Maps API key for address search functionality
# Get your API key from: https://developers.kakao.com/
NEXT_PUBLIC_KAKAO_API_KEY="your-kakao-api-key-here"
